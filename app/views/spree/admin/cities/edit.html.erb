<% content_for :page_title do %>
  <%= link_to Spree.t(:cities), spree.admin_state_cities_url(@state, @city) %> /
  <%= @city.name %>
<% end %>

<%= render partial: 'spree/admin/shared/error_messages', locals: { target: @city } %>

<%= form_for [:admin, @state, @city] do |f| %>
  <fieldset>
    <%= render partial: 'form', locals: { f: f } %>
    <%= render partial: 'spree/admin/shared/edit_resource_links' %>
  </fieldset>
<% end %>
